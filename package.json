{
  "name": "valstore",
  "version": "3.0.1-beta.0",
  "description": "Featherweight key-based value store with zero dependencies",
  "main": "pkg/dist-src/index.js",
  "scripts": {
    "build": "npm run -s clean && npx @pika/pack build",
    "build:parcel": "npm run -s clean && parcel build build.html && mv dist/src.*.js dist/index.js && mv dist/react.*.js dist/react.js",
    "clean": "rm -rf dist/ pkg/ .cache/",
    "test": "jest",
    "test:watch": "jest ---watch"
  },
  "keywords": [
    "redux",
    "central",
    "store",
    "app",
    "store",
    "value",
    "store",
    "valstore",
    "toystore"
  ],
  "author": "Vance Lucas <vance@vancelucas.com>",
  "license": "BSD-3-Clause",
  "repository": "vlucas/valstore",
  "sideEffects": false,
  "peerDependencies": {
    "preact": "^10.0.0",
    "react": "^16.8.0"
  },
  "peerDependenciesMeta": {
    "preact": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-ts-standard-pkg": "^0.9.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.13",
    "@types/react-dom": "^16.9.8",
    "htm": "^3.0.4",
    "jest": "^26.4.2",
    "microbundle": "^0.12.3",
    "preact": "^10.4.8",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {},
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
